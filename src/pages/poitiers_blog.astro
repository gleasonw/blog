---
import PostNavigator from "../components/PostNavigator.astro";
import TextBody from "../components/TextBody.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
const allPosts = await Astro.glob('../pages/posts/poitiers/*.md');
---

<BaseLayout pageTitle={"Will Goes to Poiters"}>
  <TextBody>
    A few reports from Poitiers, France.
    <PostNavigator posts={allPosts} />
  </TextBody>
</BaseLayout>
